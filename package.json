{
  "name": "stencil-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-components/stencil-components.esm.js",
      "require": "./dist/stencil-components/stencil-components.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./dist/components/*": {
      "import": "./dist/components/*",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@jest/transform": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT"
}
